if __name__=='__main__':
    tc=int(input())
    for _ in range(tcs);
        m=input()
        ar11=[int(x) for x in input().split()]
        n=input()
        arr2=[int(x) for x in input().split()]
        
        ob=Solution()
        print(ob.MedianOfArrays(arr1,arr2))
